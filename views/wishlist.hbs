<head>
    <h2 align="center">Your Wishlist!!!</h2>
</head>
<body>
    <div class="container">
        <div class="panel panel-info">
            {{# if products}}
                {{#each products}}
                    <div class="panel-heading"><a href="product/{{this.item.title}}">{{this.item.title}}</a></div>
                        <div class="panel-body">
                            <table style="width:100%">
                                <tr>
                                    <td width="20%"><img style="height:200px" src="{{this.item.url}}"></td>
                                    <td width="20%" style="text-align:center">Rs.{{this.item.price}}</td>
                                    <td width="20%">
                                        <a href="/add-to-cart/{{this.item.title}}" class="btn btn-success" style="text-align:center"role="button"><i class="fa fa-shopping-cart pull-left" style="font-size:20px" aria-hidden="true">Add to cart</i></a>
                                        <a href="/deletewish/{{this.item.title}}"> <button type="button" class="btn btn-link btn-xs pull-right"><i class="fa fa-trash-o fa-2x"></i>
                                        </button> </a>
                                    </td>
                                </tr>
                            </table>
                        </div>
                        <br>
                {{/each}}
        {{else}}
            <div class="alert alert-success alert-dismissible" id="alertMsg">
                <h3>No items in wishlist....!</h3>
            </div>
        {{/if}}
        </div>
</div>